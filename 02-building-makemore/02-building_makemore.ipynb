   "source": [
    "import math\n",
    "\n",
    "def calculate_loss(probability_func, bigram_list):\n",
    "    # TODO: Calculate the (mean) negative log likelihood of all the bigrams in bigram_list.\n",
    "    pass    # TODO: Remove this when the function is implemented."
   ]
